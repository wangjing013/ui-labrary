{
  "name": "msb-ui-library",
  "version": "0.0.1",
  "module": "dist/msb-ui.es.js",
  "browser": "dist/msb-ui.umd.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite",
    "dev:all": "npm-run-all --parallel dev:*",
    "dev:2": "vue-demi-switch 2 && cd examples/v2 && npm run serve",
    "dev:3": "vue-demi-switch 3 && cd examples/v3 && npm run dev",
    "build:2": "vue-demi-switch 2 && cd build && npm run build",
    "build:3": "vue-demi-switch 3 && run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@msbfe/live-sdk": "^0.0.2",
    "@msbfe/player-sdk": "^0.0.1"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.5",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/composition-api": "^1.7.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vitest": "^0.23.2",
    "vue": "^3.2.38",
    "vue-demi": "^0.13.11",
    "vue-promised": "^2.2.0",
    "vue-tsc": "^0.40.7"
  }
}
